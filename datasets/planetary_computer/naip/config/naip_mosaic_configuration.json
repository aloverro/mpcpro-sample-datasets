{
  "mosaics": [
    {
      "id": "latest",
      "name": "Most Recent Year",
      "description": "Latest year imagery for all sample locations",
      "cql": [
        {"op": "=", "args": [{"property": "naip:year"}, {"property": "mosaic:max(naip:year)"}]}
      ]
    },
    {
      "id": "year_2022",
      "name": "Year 2022",
      "description": "Imagery acquired in 2022",
      "cql": [
        {"op": "=", "args": [{"property": "naip:year"}, 2022]}
      ]
    }
  ],
  "renderOptions": [
    {
      "id": "natural_color",
      "name": "Natural Color (RGB)",
      "type": "raster-tile",
      "description": "Standard RGB rendering",
      "assets": ["image"],
      "asset_bidx": {"image": [1,2,3]},
      "minZoom": 4,
      "maxZoom": 20
    },
    {
      "id": "color_infrared",
      "name": "Color Infrared (CIR)",
      "type": "raster-tile",
      "description": "NIR, Red, Green composite",
      "assets": ["image"],
      "asset_bidx": {"image": [4,1,2]},
      "minZoom": 4,
      "maxZoom": 20
    }
  ],
  "defaultLocation": {"lat": 39.5, "lon": -98.35, "zoom": 4}
}
